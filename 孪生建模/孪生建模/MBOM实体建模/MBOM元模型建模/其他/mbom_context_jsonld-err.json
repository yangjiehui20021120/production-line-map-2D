      "@type": "ngsi-ld:Property",
      "rdfs:comment": "MBOM子类型标识,枚举值: MBOMRoot, Route, Takt, Process, Step"
    },
    
    "mbomCode": {
      "@id": "mbom:mbomCode",
      "@type": "ngsi-ld:Property",
      "rdfs:comment": "MBOM业务编码"
