      "@type": "ngsi-ld:Relationship",
      "rdfs:comment": "审批者"
    },
    
    "modifiedBy": {
      "@id": "mbom:modifiedBy",
      "@type": "ngsi-ld:Relationship",
      "rdfs:comment": "修改者"
    },
    
    "supersededBy": {
      "@id": "mbom:supersededBy",
      "@type": "ngsi-ld:Relationship",
      "rdfs:comment": "被替代关系"
